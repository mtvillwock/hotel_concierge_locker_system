<div class="retrieve-bag-form">
  <h2>Retrieve a bag</h2>
  <p>Please enter the id of the bag you had stored.</p>
  <%= form_for @ticket do |f| %>
  <% if @ticket.errors.any? %>
  <div id="error_explanation">
    <h2 class="error-warning">
      <%= pluralize(@ticket.errors.count, "error") %> prohibited
      this ticket from being stored:
    </h2>
    <ul>
      <% @ticket.errors.full_messages.each do |msg| %>
      <li class="error-message"><%= msg %></li>
      <% end %>
    </ul>
  </div>
  <% end %>

  <p class="ticket-id">
    <%= f.text_field :id, placeholder: 'ticket id number' %>
  </p>

  <p class="ticket-submit">
    <%= f.submit %>
  </p>

  <% end %>
</div>